//Masterfile to create the database
//We worked on this a few years ago
//Created by Kip, Saketh, and lcookiemonsterz (github name)


-- Check if the database exists
DROP DATABASE IF EXISTS bookstoredb;
-- Create the database
CREATE DATABASE IF NOT EXISTS bookstoredb;
USE bookstoredb;
-- Create the AUTHORS table
CREATE TABLE AUTHOR_INFO (
author_id INT PRIMARY KEY AUTO_INCREMENT,
author_fname VARCHAR(255),
author_lname VARCHAR(255)
);
-- Create the DISCOUNTS table
CREATE TABLE DISCOUNT (
discount_id INT PRIMARY KEY AUTO_INCREMENT,
discount_name VARCHAR(255),
discount_percentage DECIMAL(20, 2)
);
-- Create the BOOKS table
CREATE TABLE BOOK_INFO (
book_id INT PRIMARY KEY AUTO_INCREMENT,
book_title VARCHAR(255),
book_isbn VARCHAR(20),
book_publisher VARCHAR(255),
book_rating DECIMAL(3, 2),
book_listprice DECIMAL(20, 2),
discount_id INT,
FOREIGN KEY (discount_id) REFERENCES DISCOUNT(discount_id)
);
-- Create the GENRES table
CREATE TABLE GENRE (
genre_id INT PRIMARY KEY AUTO_INCREMENT,
genre_name VARCHAR(255)
);
-- Create a join table to associate books with genres (many-to-many relationship)
CREATE TABLE BOOKS_GENRES (
book_id INT,
genre_id INT,
PRIMARY KEY (book_id, genre_id),
FOREIGN KEY (book_id) REFERENCES BOOK_INFO(book_id),
FOREIGN KEY (genre_id) REFERENCES GENRE(genre_id)
);
-- Create a join table to associate authors with books (many-to-many relationship)
CREATE TABLE AUTHORS_BOOKS (
author_id INT,
book_id INT,
PRIMARY KEY (author_id, book_id),
FOREIGN KEY (author_id) REFERENCES AUTHOR_INFO(author_id),
FOREIGN KEY (book_id) REFERENCES BOOK_INFO(book_id)
);
-- Create the STORE_INFO table
CREATE TABLE STORE_INFO (
store_id INT PRIMARY KEY AUTO_INCREMENT,
store_name VARCHAR(255),
store_location VARCHAR(255)
);
-- Create the INVENTORY table to associate stores with books
CREATE TABLE INVENTORY (
store_id INT,
book_id INT,
PRIMARY KEY (store_id, book_id),
FOREIGN KEY (store_id) REFERENCES STORE_INFO(store_id),
FOREIGN KEY (book_id) REFERENCES BOOK_INFO(book_id)
);
-- Create the CUSTOMER_INFO table
CREATE TABLE CUSTOMER_INFO (
cust_id INT PRIMARY KEY AUTO_INCREMENT,
cust_fname VARCHAR(255),
cust_lname VARCHAR(255),
cust_email VARCHAR(255),
cust_phone VARCHAR(15),
cust_address VARCHAR(255),
cust_gold_member_status TINYINT
);
-- Create the PAYMENTS table
CREATE TABLE PAYMENT (
payment_id INT PRIMARY KEY AUTO_INCREMENT,
payment_amount DECIMAL(10, 2),
payment_type VARCHAR(255),
payment_date DATETIME
);
-- Create the ORDERS table
CREATE TABLE `ORDER` (
order_id INT PRIMARY KEY AUTO_INCREMENT,
cust_id INT,
store_id INT,
payment_id INT,
FOREIGN KEY (cust_id) REFERENCES CUSTOMER_INFO(cust_id),
FOREIGN KEY (store_id) REFERENCES STORE_INFO(store_id),
FOREIGN KEY (payment_id) REFERENCES PAYMENT(payment_id)
);
CREATE TABLE `ORDERLINE` (
orderline_id INT PRIMARY KEY AUTO_INCREMENT,
order_id INT,
book_id INT,
orderline_qty INT,
orderline_book_price DECIMAL(20,2),
FOREIGN KEY (book_id) REFERENCES BOOK_INFO(book_id),
FOREIGN KEY (order_id) REFERENCES `ORDER`(order_id)
);
-- Create the REVIEWS table
CREATE TABLE REVIEW (
review_id INT PRIMARY KEY AUTO_INCREMENT,
cust_id INT,
book_id INT,
review_score INT,
review_title TEXT,
review_comment TEXT,
FOREIGN KEY (cust_id) REFERENCES CUSTOMER_INFO(cust_id),
FOREIGN KEY (book_id) REFERENCES BOOK_INFO(book_id)
);
