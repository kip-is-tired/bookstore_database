-- saketh - calculte appropriate orderline prices
UPDATE ORDERLINE
JOIN BOOK_INFO bi ON ORDERLINE.book_id = bi.book_id
JOIN DISCOUNT d ON bi.discount_id = d.discount_id
SET ORDERLINE.orderline_book_price = FORMAT((bi.book_listprice - (bi.book_listprice
* d.discount_percentage / 100)),2);
-- saketh - update appropriate payment amounts
UPDATE PAYMENT p
SET p.payment_amount = (
SELECT
SUM(ol.orderline_book_price * ol.orderline_qty) AS order_total_amount
FROM
`ORDER` o
JOIN
ORDERLINE ol ON o.order_id = ol.order_id
WHERE
p.payment_id = o.payment_id
GROUP BY
o.order_id
);
-- saketh- add view to see all aothrus of book
CREATE VIEW BooksWithAuthors AS
SELECT
b.book_id,
b.book_title,
b.book_isbn,
b.book_publisher,
b.book_rating,
b.book_listprice,
GROUP_CONCAT(a.author_fname, ' ', a.author_lname SEPARATOR ', ') AS authors
FROM
BOOK_INFO b
JOIN
AUTHORS_BOOKS ab ON b.book_id = ab.book_id
JOIN
AUTHOR_INFO a ON ab.author_id = a.author_id
GROUP BY
b.book_id, b.book_title, b.book_isbn, b.book_publisher, b.book_rating,
b.book_listprice;
--fardin 7,8 :
-- Trigger on insert. An author who is inserted into Author_INFO will have a book
precreated for them in the Authors_BOOKS table with a place holder value
DELIMITER //
CREATE TRIGGER InsertNewAuthorBook
AFTER INSERT ON AUTHOR_INFO
FOR EACH ROW
BEGIN
DECLARE authorExists INT;
-- Check if the author_id exists in AUTHORS_BOOKS
SELECT COUNT(*) INTO authorExists FROM AUTHORS_BOOKS WHERE author_id =
NEW.author_id;
IF authorExists = 0 THEN
-- Insert a valid book_id in AUTHORS_BOOKS associated with the author
INSERT INTO AUTHORS_BOOKS (author_id, book_id) VALUES (NEW.author_id,
(SELECT book_id FROM BOOK_INFO LIMIT 1)); -- Selects a valid book_id from BOOK_INFO
END IF;
END;
//
DELIMITER ;
-- Insert an author into AUTHOR_INFO
INSERT INTO AUTHOR_INFO (author_id, author_fname, author_lname) VALUES (100, 'New',
'Author');
SELECT * FROM AUTHORS_BOOKS
DELIMITER //
CREATE PROCEDURE UpdateCapitalizedNames()
BEGIN
DECLARE done INT DEFAULT FALSE;
DECLARE firstName VARCHAR(255);
DECLARE lastName VARCHAR(255);
DECLARE newFirstName VARCHAR(255);
DECLARE newLastName VARCHAR(255);
DECLARE cur CURSOR FOR SELECT author_fname, author_lname FROM AUTHOR_INFO;
DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = TRUE;
OPEN cur;
read_loop: LOOP
FETCH cur INTO firstName, lastName;
IF done THEN
LEAVE read_loop;
END IF;
-- Check if the first letter of fname and lname are not capitalized
IF ASCII(LEFT(firstName, 1)) NOT BETWEEN ASCII('A') AND ASCII('Z') THEN
SET newFirstName = CONCAT(UPPER(LEFT(firstName, 1)),
SUBSTRING(firstName, 2));
ELSE
SET newFirstName = firstName;
END IF;
IF ASCII(LEFT(lastName, 1)) NOT BETWEEN ASCII('A') AND ASCII('Z') THEN
SET newLastName = CONCAT(UPPER(LEFT(lastName, 1)), SUBSTRING(lastName,
2));
ELSE
SET newLastName = lastName;
END IF;
-- Update the row with capitalized names
UPDATE AUTHOR_INFO SET author_fname = newFirstName, author_lname =
newLastName WHERE author_fname = firstName AND author_lname = lastName;
END LOOP;
CLOSE cur;
END;
//
DELIMITER ;
INSERT INTO AUTHOR_INFO (author_id, author_fname, author_lname) VALUES (200,
'lower', 'case');
SELECT * FROM AUTHOR_INFO;
CALL UpdateCapitalizedNames();
SELECT * FROM AUTHOR_INFO;
-- backup triggers and procedurers
DELIMITER //
CREATE PROCEDURE CalculateTotalAmount(IN custID INT, OUT totalAmount DECIMAL(10,
2))
BEGIN
SELECT SUM(ol.orderline_qty * ol.orderline_book_price) INTO totalAmount
FROM `ORDER` o
JOIN `ORDERLINE` ol ON o.order_id = ol.order_id
WHERE o.cust_id = custID;
END //
DELIMITER ;
DELIMITER //
CREATE PROCEDURE UpdateBookRatings(IN percentage DECIMAL(5, 2))
BEGIN
DECLARE done INT DEFAULT FALSE;
DECLARE bookID INT;
DECLARE cur CURSOR FOR SELECT book_id FROM BOOK_INFO;
DECLARE CONTINUE HANDLER FOR NOT FOUND SET done = TRUE;
OPEN cur;
read_loop: LOOP
FETCH cur INTO bookID;
IF done THEN
LEAVE read_loop;
END IF;
UPDATE BOOK_INFO SET book_rating = book_rating + (book_rating *
percentage / 100) WHERE book_id = bookID;
END LOOP;
CLOSE cur;
END //
DELIMITER ;
DELIMITER //
CREATE TRIGGER UpdateGenreRating
AFTER INSERT ON REVIEW
FOR EACH ROW
BEGIN
DECLARE avgRating DECIMAL(3, 2);
SELECT AVG(review_score) INTO avgRating FROM REVIEW WHERE book_id =
NEW.book_id;
UPDATE GENRE g
JOIN BOOKS_GENRES bg ON g.genre_id = bg.genre_id
SET g.genre_avg_rating = avgRating
WHERE bg.book_id = NEW.book_id;
END;
DELIMITER ;

